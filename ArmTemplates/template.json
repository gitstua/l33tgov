{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "applicationGateways_appgw2_name": {
            "defaultValue": "appgw2",
            "type": "String"
        },
        "virtualNetworks_se_abs_vnet_aueast1_externalid": {
            "defaultValue": "/subscriptions/e7752f09-24a1-4b14-9931-6a56d4f6109d/resourceGroups/seAbsDipaRG/providers/Microsoft.Network/virtualNetworks/se-abs-vnet-aueast1",
            "type": "String"
        },
        "publicIPAddresses_se_abs_aueast_pip1_externalid": {
            "defaultValue": "/subscriptions/e7752f09-24a1-4b14-9931-6a56d4f6109d/resourceGroups/seAbsDipaRG/providers/Microsoft.Network/publicIPAddresses/se-abs-aueast-pip1",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/applicationGateways",
            "apiVersion": "2019-09-01",
            "name": "[parameters('applicationGateways_appgw2_name')]",
            "location": "australiaeast",
            "properties": {
                "sku": {
                    "name": "WAF_v2",
                    "tier": "WAF_v2",
                    "capacity": 1
                },
                "gatewayIPConfigurations": [
                    {
                        "name": "appGatewayIpConfig",
                        "properties": {
                            "subnet": {
                                "id": "[concat(parameters('virtualNetworks_se_abs_vnet_aueast1_externalid'), '/subnets/default')]"
                            }
                        }
                    }
                ],
                "sslCertificates": [
                    {
                        "name": "website1.devopsint.com",
                        "properties": {}
                    }
                ],
                "trustedRootCertificates": [],
                "frontendIPConfigurations": [
                    {
                        "name": "appGwPublicFrontendIp",
                        "properties": {
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[parameters('publicIPAddresses_se_abs_aueast_pip1_externalid')]"
                            }
                        }
                    },
                    {
                        "name": "appGwPrivateFrontendIp",
                        "properties": {
                            "privateIPAddress": "10.2.0.4",
                            "privateIPAllocationMethod": "Static",
                            "subnet": {
                                "id": "[concat(parameters('virtualNetworks_se_abs_vnet_aueast1_externalid'), '/subnets/default')]"
                            }
                        }
                    }
                ],
                "frontendPorts": [
                    {
                        "name": "port_443",
                        "properties": {
                            "port": 443
                        }
                    }
                ],
                "backendAddressPools": [
                    {
                        "name": "seabsdipablob.z24.web.core.windows.net",
                        "properties": {
                            "backendAddresses": [
                                {
                                    "fqdn": "seabsdipablob.z24.web.core.windows.net"
                                }
                            ]
                        }
                    }
                ],
                "backendHttpSettingsCollection": [
                    {
                        "name": "http-setting-backend-https",
                        "properties": {
                            "port": 443,
                            "protocol": "Https",
                            "cookieBasedAffinity": "Disabled",
                            "pickHostNameFromBackendAddress": true,
                            "affinityCookieName": "ApplicationGatewayAffinity",
                            "requestTimeout": 20,
                            "probe": {
                                "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_appgw2_name')), '/probes/http-setting-backend-httpsc2357347-e290-4ce5-9f7a-08ab85e0c018')]"
                            }
                        }
                    }
                ],
                "httpListeners": [
                    {
                        "name": "listener1",
                        "properties": {
                            "frontendIPConfiguration": {
                                "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_appgw2_name')), '/frontendIPConfigurations/appGwPublicFrontendIp')]"
                            },
                            "frontendPort": {
                                "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_appgw2_name')), '/frontendPorts/port_443')]"
                            },
                            "protocol": "Https",
                            "sslCertificate": {
                                "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_appgw2_name')), '/sslCertificates/website1.devopsint.com')]"
                            },
                            "hostNames": [],
                            "requireServerNameIndication": false
                        }
                    }
                ],
                "urlPathMaps": [],
                "requestRoutingRules": [
                    {
                        "name": "rule1",
                        "properties": {
                            "ruleType": "Basic",
                            "httpListener": {
                                "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_appgw2_name')), '/httpListeners/listener1')]"
                            },
                            "backendAddressPool": {
                                "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_appgw2_name')), '/backendAddressPools/seabsdipablob.z24.web.core.windows.net')]"
                            },
                            "backendHttpSettings": {
                                "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_appgw2_name')), '/backendHttpSettingsCollection/http-setting-backend-https')]"
                            },
                            "rewriteRuleSet": {
                                "id": "[concat(resourceId('Microsoft.Network/applicationGateways', parameters('applicationGateways_appgw2_name')), '/rewriteRuleSets/rewriteset1')]"
                            }
                        }
                    }
                ],
                "probes": [
                    {
                        "name": "http-setting-backend-httpsc2357347-e290-4ce5-9f7a-08ab85e0c018",
                        "properties": {
                            "protocol": "Https",
                            "path": "/",
                            "interval": 30,
                            "timeout": 30,
                            "unhealthyThreshold": 3,
                            "pickHostNameFromBackendHttpSettings": true,
                            "minServers": 0,
                            "match": {
                                "statusCodes": [
                                    "200-399"
                                ]
                            }
                        }
                    }
                ],
                "rewriteRuleSets": [
                    {
                        "name": "rewriteset1",
                        "properties": {
                            "rewriteRules": [
                                {
                                    "ruleSequence": 100,
                                    "conditions": [],
                                    "name": "NewRewrite",
                                    "actionSet": {
                                        "requestHeaderConfigurations": [],
                                        "responseHeaderConfigurations": [
                                            {
                                                "headerName": "Strict-Transport-Security",
                                                "headerValue": "max-age=31536000; includeSubDomains"
                                            },
                                            {
                                                "headerName": "X-Frame-Options",
                                                "headerValue": "SAMEORIGIN"
                                            },
                                            {
                                                "headerName": "X-Content-Type-Options",
                                                "headerValue": "nosniff"
                                            },
                                            {
                                                "headerName": "Server"
                                            },
                                            {
                                                "headerName": "Content-Security-Policy",
                                                "headerValue": "default-src 'none'; script-src 'self'; connect-src 'self'; img-src 'self'; style-src 'self';"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                ],
                "redirectConfigurations": [],
                "sslPolicy": {
                    "policyType": "Predefined",
                    "policyName": "AppGwSslPolicy20170401S"
                },
                "webApplicationFirewallConfiguration": {
                    "enabled": true,
                    "firewallMode": "Detection",
                    "ruleSetType": "OWASP",
                    "ruleSetVersion": "3.0",
                    "disabledRuleGroups": [],
                    "requestBodyCheck": true,
                    "maxRequestBodySizeInKb": 128,
                    "fileUploadLimitInMb": 100
                },
                "enableHttp2": true
            }
        }
    ]
}